<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="example.com.healthpetapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthPetApp">
        <activity
            android:name=".ver_RateUs"
            android:exported="false" />
        <activity
            android:name=".ui.contactos.ContactsActivity"
            android:exported="false" />
        <activity
            android:name=".About"
            android:exported="true" />
        <activity
            android:name=".Language"
            android:exported="true" />
        <activity
            android:name=".Help"
            android:exported="true" />
        <activity
            android:name=".VerCitas"
            android:exported="false"
            android:label="@string/view_dates"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".add_pet"
            android:exported="true"
            android:label="@string/add_pet"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".Settings"
            android:exported="true"
            android:label="@string/settings"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".Profile"
            android:exported="true"
            android:label="@string/profile_activity"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".MedicalHistory"
            android:exported="true"
            android:label="@string/medicalhistory_activity"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".MyContact"
            android:exported="true"
            android:label="@string/mycontact_activity"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".AddContact"
            android:exported="true"
            android:label="@string/add_contact"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".shedule_appointment"
            android:exported="true"
            android:label="@string/shedule_appointment"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:label="@string/login" />
        <activity
            android:name=".TwoStepVerification"
            android:exported="true"
            android:label="@string/Two_Step_Verification" />
        <activity
            android:name=".RegisterActivity"
            android:exported="true"
            android:label="@string/Welcome_to_healthpet"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".MyAppointment"
            android:exported="true"
            android:label="@string/myappointment_activity"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".MenuActivity"
            android:exported="true"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.HealthPetApp.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>